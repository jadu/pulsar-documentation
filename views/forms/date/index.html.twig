{% extends 'docs.html.twig' %}

{% set heading = 'Date' %}

{%
    set breadcrumbs = [
        {'label': 'Home', 'href': '/'},
        {'label': 'Forms', 'data-toggle-nav': '#forms'},
        {'label': 'Date', 'href': '#'}
    ]
%}

{% set tab_new %}
    {% include 'forms/date/new.html.twig' %}
{% endset %}
{% set tab_deprecated %}
    {% include 'forms/date/deprecated.html.twig' %}
{% endset %}

{%
    set tabs_content = [
        {
          "id"    : "new",
          "class" : "ui-report",
          "label" : "New",
          "src"   : tab_new,
          "active": true
        },
        {
          "id"    : "deprecated",
          "class" : "ui-report",
          "label" : "Deprecated",
          "src"   : tab_deprecated
        }
    ]
%}
